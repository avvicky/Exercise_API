{
    "version":2,
    "builds": [
        {
        "src": "./index.js",
        "use": "@vercel/node"
    }
    ],
    "routes": [
        {
            "src":"/(.*)",
            "dest": "/"
        },
        { "handle": "filesystem" },
        { "src": "/views/(.*)", "dest": "/views/$1" }
    ]
}